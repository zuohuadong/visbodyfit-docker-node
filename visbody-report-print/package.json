{
    "name": "visbody-report-print",
    "version": "1.0.3",
    "description": "维塑体测报告打印服务",
    "scripts": {
        "start": "ts-node -r tsconfig-paths/register src/main.ts",
        "start:watch": "nodemon",
        "debug": "node --inspect-brk -r ts-node/register src/main.ts",
        "debug:watch": "nodemon --config nodemon-debug.json",
        "check": "tslint -p tsconfig.json -c tslint.json",
        "fix": "tslint -p tsconfig.json -c tslint.json --fix"
    },
    "author": "yangsheng",
    "license": "MIT",
    "private": false,
    "dependencies": {
        "@nestjs/common": "^5.3.0",
        "@nestjs/core": "^5.3.0",
        "express-rate-limit": "^3.3.1",
        "ioredis": "^4.2.0",
        "moment": "^2.22.2",
        "morgan": "^1.9.1",
        "node-schedule": "^1.3.0",
        "puppeteer": "^1.8.0",
        "rate-limit-redis": "^1.5.0",
        "reflect-metadata": "^0.1.12",
        "rxjs": "^6.2.2",
        "typescript": "^2.9.2"
    },
    "devDependencies": {
        "@types/ioredis": "4.0.0",
        "@types/morgan": "^1.7.35",
        "@types/node": "^10.5.2",
        "@types/puppeteer": "^1.6.3",
        "nodemon": "^1.18.3",
        "ts-node": "^7.0.0",
        "tsconfig-paths": "^3.4.2",
        "tslint": "^5.11.0"
    }
}
